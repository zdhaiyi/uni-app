  "app-harmony": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    },
    "uniStatistics": {
      "enable": false
    }
  },
  "locale": "zh-Hans",
  "screenOrientation": [
    "portrait-primary"
  ],
  "launch_path": "__uniappview.html"
}